<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swpu.mapper.CommodityMapper">

    <select id="findByName" parameterType="String" resultType="com.swpu.entity.Commodity">
    SELECT id,name,type,createTime,description,category,price,pic_path,user_id,count,updateTime
    FROM commodity
    WHERE name = #{name}
    </select>





</mapper>
